{
  "expo": {
    "name": "PrestoHoy",
    "slug": "prestohoy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.carlosp04.prestohoy",
      "buildNumber": "1.0.115",
      "icon": "./src/assets/logo.png",
      "infoPlist": {
        "NSCameraUsageDescription": "Necesitamos acceder a la camara para poder escanear los codigos QR de los clientes y hacer transacciones."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carlosp04.prestohoy",
      "permissions": ["android.permission.CAMERA"],
      "config": {
        "googlePlay": {
          "permissions": ["com.google.android.gms.permission.CAMERA"]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "ed1a88e8-3f27-4c41-91f8-a3c50058765c"
      }
    },
    "owner": "carlosp04",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "expo-updates",
        {
          "username": "account-username"
        }
      ]
    ]
  }
}
